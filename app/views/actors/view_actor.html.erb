  <div class="page-header">
    <h1>View actor</h1>
  </div>

  <dl class="dl-horizontal">
    <dt>Name:</dt>
    <dd><p><%= @actor.name %></p></dd>
    <dt>DOB:</dt>
    <dd><p><%= @actor.dob %></p></dd>
    <dt>Bio:</dt>
    <dd><p><%= @actor.bio %></p></dd>
    <dt>Image</dt>
    <dd><img width="400" src="<%= @actor.image_url %>"></dd>
  </dl>

  <a class="btn btn-primary" href="/actors">Back</a>
